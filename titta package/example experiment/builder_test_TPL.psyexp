        <Param val="# Ensure the current image exists in Pro Lab media list; otherwise upload it&amp;#10;#import os&amp;#10;file_name = image_file                     # e.g., &quot;im1.jpeg&quot; from conditions&amp;#10;base_name = Path(file_name).stem  # &quot;im1&quot; from &quot;im1.jpeg&quot;&amp;#10;&amp;#10;# Lookup or upload media&amp;#10;if base_name not in media_cache:&amp;#10;    # If not found, upload; if found, get its media_id&amp;#10;    if not ttl.find_media(file_name):&amp;#10;        info = ttl.upload_media(file_name, &quot;image&quot;)  # returns dict info&amp;#10;        media_cache[base_name] = info['media_id']&amp;#10;        print(f'Uploaded media {file_name}')&amp;#10;    else:&amp;#10;        uploaded_media = ttl.list_media()['media_list']&amp;#10;        for m in uploaded_media:&amp;#10;            if base_name == m['media_name']:&amp;#10;                media_cache[base_name] = m['media_id']&amp;#10;                break&amp;#10;&amp;#10;# Optionally add AOI to the first image once&amp;#10;if not aoi_added:&amp;#10;    aoi_name = 'test'&amp;#10;    aoi_color = 'AAC333'  # hex without '#'&amp;#10;    vertices = ((100, 100),&amp;#10;                (100, 200),&amp;#10;                (200, 200),&amp;#10;                (200, 100))&amp;#10;    ttl.add_aois_to_image(&amp;#10;        media_cache[base_name],&amp;#10;        aoi_name, aoi_color,&amp;#10;        vertices,&amp;#10;        tag_name='test_tag',&amp;#10;        group_name='test_group'&amp;#10;    )&amp;#10;    aoi_added = True&amp;#10;&amp;#10;# Prepare onset/offset logging for this trial&amp;#10;t_onset = None&amp;#10;t_offset = None&amp;#10;curr_media_id = media_cache[base_name]" valType="extendedCode" updates="constant" name="Begin Routine"/>
